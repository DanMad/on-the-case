{
  "name": "on-the-case",
  "version": "2.1.0",
  "description": "A module that extends JavaScript's String object with additional case-conversion support",
  "homepage": "https://www.npmjs.com/package/on-the-case",
  "bugs": "https://github.com/DanMad/on-the-case/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanMad/on-the-case.git"
  },
  "license": "MIT",
  "author": "DanMad",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn install && husky install && chmod +x .husky/*",
    "bootstrap-frozen": "rm -rf node_modules && yarn install --frozen-lockfile && husky install && chmod +x .husky/*",
    "build": "rm -rf dist && webpack",
    "format": "sort-package-json && prettier --write \"**/*\"",
    "test": "jest",
    "test-coverage": "jest --coverage --colors"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sort-package-json": "^1.57.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
